FROM image:latest

# Compliant
ENV APIKEY ""
ENV APIKEY "{{ APIKEY }}"
ENV APIKEY {{ APIKEY }}
ENV APIKEY {APIKEY}
ENV APIKEY $APIKEY
ENV APIKEY=
ENV APIKEY="{{ APIKEY }}"
ENV APIKEY={{ APIKEY }}
ENV APIKEY={APIKEY}
ENV APIKEY=$APIKEY

# Noncompliant
ENV APIKEY YXNkZmZmZmZm_HARDcoded01
ENV APIKEY=YXNkZmZmZmZm_HARDcoded02
ENV APIKEY="YXNkZmZmZmZm_HARDcoded03"
